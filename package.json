{
	"name": "todo",
	"version": "1.0.0",
	"description": "basic todo app written in react",
	"main": "index.js",
	"scripts": {
		"dev": "webpack-dev-server --inline --hot",
		"build": "NODE_ENV=production webpack --progress",
		"cleanup": "rm README.md && sudo rm -r .git"
	},
	"babel": {
		"presets": [
			"react",
